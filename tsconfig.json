{
 "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    
    "paths": {
      "@/*": ["./*"]
    }
  },
  "typeRoots": ["./node_modules/@types", "./src/types"],
  "types": ["nativewind/types"],
  // Oth
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}

